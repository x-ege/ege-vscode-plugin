{
	"name": "ege",
	"displayName": "ege",
	"description": "Auto configuration for ege (https://xege.org)",
	"version": "1.0.0",
	"icon": "images/logo.png",
	"publisher": "wysaid",
	"repository": {
		"type": "git",
		"url": "https://github.com/wysaid/vscode-ege"
	},
	"homepage": "https://github.com/wysaid/vscode-ege",
	"engines": {
		"vscode": "^1.63.0"
	},
	"categories": [
		"Education",
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"onCommand:ege.setupProject",
		"onCommand:ege.buildAndRunCurrentFile",
		"onCommand:ege.setupGlobal",
		"onCommand:ege.cleanupCaches",
		"onCommand:ege.openCacheDir"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ege.setupProject",
				"title": "%command.setupProject%"
			},
			{
				"command": "ege.setupProjectWithEgeSrc",
				"title": "%command.setupProjectWithSource%"
			},
			{
				"command": "ege.setupGlobal",
				"title": "%command.setupGlobal%"
			},
			{
				"command": "ege.cleanupCaches",
				"title": "%command.cleanupCaches%"
			},
			{
				"command": "ege.openCacheDir",
				"title": "%command.openCacheDir%"
			},
			{
				"command": "ege.buildAndRunCurrentFile",
				"title": "%command.buildAndRunCurrentFile%"
			}
		],
		"configuration": {
			"type": "object",
			"title": "%title.egeConfiguration%",
			"properties": {
				"ege.update_url": {
					"type": "string",
					"description": "%config.updateUrl.description%",
					"scope": "resource"
				},
				"ege.showEditorContextMenu": {
					"type": "boolean",
					"default": true,
					"description": "%config.showEditorContextMenu.description%",
					"scope": "resource"
				},
				"ege.explorerContextMenu": {
					"type": "boolean",
					"default": true,
					"description": "%config.explorerContextMenu.description%",
					"scope": "resource"
				}
			}
		},
		"keybindings": [],
		"menus": {
			"editor/context": [
				{
					"command": "ege.setupProject",
					"when": "!inOutput && config.ege.showEditorContextMenu",
					"group": "1_modification"
				},
				{
					"command": "ege.buildAndRunCurrentFile",
					"when": "!inOutput && config.ege.showEditorContextMenu && resourceExtname == .cpp",
					"group": "1_modification"
				}
			],
			"explorer/context": [
				{
					"command": "ege.setupProject",
					"when": "config.ege.explorerContextMenu && explorerResourceIsFolder",
					"group": "1_modification"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"vscode:prepublish": "npm run package",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"watch-compile": "tsc -p ./ -w"
	},
	"commands": {
		"file": "echo ${file}",
		"fileDirname": "echo ${fileDirname}"
	},
	"devDependencies": {
		"@types/decompress": "^4.2.4",
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.61.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.0.0",
		"mocha": "^8.1.3",
		"ts-loader": "^9.3.1",
		"typescript": "^4.4.3",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
		"decompress": "^4.2.1",
		"decompress-zip": "^0.3.3",
		"fs-extra": "^10.0.0",
		"glob": "^7.2.0",
		"https": "^1.0.0",
		"iconv-lite": "^0.6.3"
	}
	,
	"overrides": {
		"minimatch": "3.1.2"
	}
}